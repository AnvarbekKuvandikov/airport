        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="700.0" styleClass="line">
            <children>
                <ImageView fx:id="logo" fitHeight="55.0" fitWidth="250.0">
                    <image>
                        <Image url="@img/S7-airline-logo.png"/>
                    </image>
                    <effect>
                        <Blend mode="RED" opacity="0.49"/>
